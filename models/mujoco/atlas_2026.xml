    <jointpos name="l_hip_pitch_pos" joint="l_hip_pitch"/>
    <jointpos name="l_knee_pitch_pos" joint="l_knee_pitch"/>
    <jointpos name="l_knee_roll_pos" joint="l_knee_roll"/>
    <jointpos name="l_ankle_pitch_pos" joint="l_ankle_pitch"/>
    <jointpos name="l_ankle_roll_pos" joint="l_ankle_roll"/>
    <jointpos name="l_ankle_yaw_pos" joint="l_ankle_yaw"/>

    <jointpos name="r_hip_yaw_pos" joint="r_hip_yaw"/>
    <jointpos name="r_hip_roll_pos" joint="r_hip_roll"/>
    <jointpos name="r_hip_pitch_pos" joint="r_hip_pitch"/>
    <jointpos name="r_knee_pitch_pos" joint="r_knee_pitch"/>
    <jointpos name="r_knee_roll_pos" joint="r_knee_roll"/>
    <jointpos name="r_ankle_pitch_pos" joint="r_ankle_pitch"/>
    <jointpos name="r_ankle_roll_pos" joint="r_ankle_roll"/>
    <jointpos name="r_ankle_yaw_pos" joint="r_ankle_yaw"/>

    <!-- Joint velocity sensors -->
    <jointvel name="l_hip_pitch_vel" joint="l_hip_pitch"/>
    <jointvel name="l_knee_pitch_vel" joint="l_knee_pitch"/>
    <jointvel name="r_hip_pitch_vel" joint="r_hip_pitch"/>
    <jointvel name="r_knee_pitch_vel" joint="r_knee_pitch"/>
  </sensor>

