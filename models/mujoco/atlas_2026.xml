                    <geom type="cylinder" size="0.04 0.02" material="atlas_light"/>

                    <body name="r_foot_link" pos="0 0 -0.03">
                      <joint name="r_ankle_yaw" class="ankle" type="hinge" axis="0 0 1" range="-0.52 0.52"/>
                      <inertial pos="0.05 0 -0.025" mass="1.5" diaginertia="0.003 0.01 0.01"/>
                      <geom type="box" size="0.14 0.06 0.025" pos="0.05 0 -0.025" material="atlas_orange"/>
                      <site name="r_foot_site" pos="0.05 0 -0.05"/>
                    </body>
                  </body>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>

    </body>
  </worldbody>

  <!-- ============================================================= -->
  <!-- ACTUATORS - 56 DOF Total -->
  <!-- ============================================================= -->
  <actuator>
    <!-- Torso actuators (6 DOF) -->
    <motor name="waist_yaw_motor" joint="waist_yaw" gear="400" ctrlrange="-1 1"/>
    <motor name="waist_pitch_motor" joint="waist_pitch" gear="400" ctrlrange="-1 1"/>
    <motor name="waist_roll_motor" joint="waist_roll" gear="400" ctrlrange="-1 1"/>
    <motor name="chest_yaw_motor" joint="chest_yaw" gear="300" ctrlrange="-1 1"/>
    <motor name="chest_pitch_motor" joint="chest_pitch" gear="300" ctrlrange="-1 1"/>
    <motor name="chest_roll_motor" joint="chest_roll" gear="300" ctrlrange="-1 1"/>

    <!-- Head actuators (3 DOF) -->
    <motor name="neck_yaw_motor" joint="neck_yaw" gear="30" ctrlrange="-1 1"/>
    <motor name="neck_pitch_motor" joint="neck_pitch" gear="30" ctrlrange="-1 1"/>
    <motor name="neck_roll_motor" joint="neck_roll" gear="30" ctrlrange="-1 1"/>

    <!-- Left arm actuators (8 DOF) -->
    <motor name="l_shoulder_pitch_motor" joint="l_shoulder_pitch" gear="200" ctrlrange="-1 1"/>
    <motor name="l_shoulder_roll_motor" joint="l_shoulder_roll" gear="200" ctrlrange="-1 1"/>
    <motor name="l_shoulder_yaw_motor" joint="l_shoulder_yaw" gear="150" ctrlrange="-1 1"/>
    <motor name="l_elbow_pitch_motor" joint="l_elbow_pitch" gear="100" ctrlrange="-1 1"/>
    <motor name="l_elbow_roll_motor" joint="l_elbow_roll" gear="100" ctrlrange="-1 1"/>
    <motor name="l_wrist_yaw_motor" joint="l_wrist_yaw" gear="50" ctrlrange="-1 1"/>
    <motor name="l_wrist_pitch_motor" joint="l_wrist_pitch" gear="50" ctrlrange="-1 1"/>
    <motor name="l_wrist_roll_motor" joint="l_wrist_roll" gear="50" ctrlrange="-1 1"/>

    <!-- Left hand actuators (7 DOF) -->
    <motor name="l_thumb_base_motor" joint="l_thumb_base" gear="20" ctrlrange="-1 1"/>
    <motor name="l_thumb_flex_motor" joint="l_thumb_flex" gear="20" ctrlrange="-1 1"/>
    <motor name="l_index_flex_motor" joint="l_index_flex" gear="20" ctrlrange="-1 1"/>
    <motor name="l_middle_flex_motor" joint="l_middle_flex" gear="20" ctrlrange="-1 1"/>
    <motor name="l_ring_flex_motor" joint="l_ring_flex" gear="20" ctrlrange="-1 1"/>
    <motor name="l_pinky_flex_motor" joint="l_pinky_flex" gear="20" ctrlrange="-1 1"/>

    <!-- Right arm actuators (8 DOF) -->
    <motor name="r_shoulder_pitch_motor" joint="r_shoulder_pitch" gear="200" ctrlrange="-1 1"/>
    <motor name="r_shoulder_roll_motor" joint="r_shoulder_roll" gear="200" ctrlrange="-1 1"/>
    <motor name="r_shoulder_yaw_motor" joint="r_shoulder_yaw" gear="150" ctrlrange="-1 1"/>
    <motor name="r_elbow_pitch_motor" joint="r_elbow_pitch" gear="100" ctrlrange="-1 1"/>
    <motor name="r_elbow_roll_motor" joint="r_elbow_roll" gear="100" ctrlrange="-1 1"/>
    <motor name="r_wrist_yaw_motor" joint="r_wrist_yaw" gear="50" ctrlrange="-1 1"/>
    <motor name="r_wrist_pitch_motor" joint="r_wrist_pitch" gear="50" ctrlrange="-1 1"/>
    <motor name="r_wrist_roll_motor" joint="r_wrist_roll" gear="50" ctrlrange="-1 1"/>

    <!-- Right hand actuators (8 DOF) -->
    <motor name="r_thumb_base_motor" joint="r_thumb_base" gear="20" ctrlrange="-1 1"/>
    <motor name="r_thumb_flex_motor" joint="r_thumb_flex" gear="20" ctrlrange="-1 1"/>
    <motor name="r_thumb_tip_motor" joint="r_thumb_tip" gear="15" ctrlrange="-1 1"/>
    <motor name="r_index_flex_motor" joint="r_index_flex" gear="20" ctrlrange="-1 1"/>
    <motor name="r_middle_flex_motor" joint="r_middle_flex" gear="20" ctrlrange="-1 1"/>
    <motor name="r_ring_flex_motor" joint="r_ring_flex" gear="20" ctrlrange="-1 1"/>
    <motor name="r_pinky_flex_motor" joint="r_pinky_flex" gear="20" ctrlrange="-1 1"/>

    <!-- Left leg actuators (8 DOF) -->
    <motor name="l_hip_yaw_motor" joint="l_hip_yaw" gear="350" ctrlrange="-1 1"/>
    <motor name="l_hip_roll_motor" joint="l_hip_roll" gear="350" ctrlrange="-1 1"/>
    <motor name="l_hip_pitch_motor" joint="l_hip_pitch" gear="450" ctrlrange="-1 1"/>
    <motor name="l_knee_pitch_motor" joint="l_knee_pitch" gear="500" ctrlrange="-1 1"/>
    <motor name="l_knee_roll_motor" joint="l_knee_roll" gear="150" ctrlrange="-1 1"/>
    <motor name="l_ankle_pitch_motor" joint="l_ankle_pitch" gear="200" ctrlrange="-1 1"/>
    <motor name="l_ankle_roll_motor" joint="l_ankle_roll" gear="150" ctrlrange="-1 1"/>
    <motor name="l_ankle_yaw_motor" joint="l_ankle_yaw" gear="100" ctrlrange="-1 1"/>

    <!-- Right leg actuators (8 DOF) -->
    <motor name="r_hip_yaw_motor" joint="r_hip_yaw" gear="350" ctrlrange="-1 1"/>
    <motor name="r_hip_roll_motor" joint="r_hip_roll" gear="350" ctrlrange="-1 1"/>
    <motor name="r_hip_pitch_motor" joint="r_hip_pitch" gear="450" ctrlrange="-1 1"/>
    <motor name="r_knee_pitch_motor" joint="r_knee_pitch" gear="500" ctrlrange="-1 1"/>
    <motor name="r_knee_roll_motor" joint="r_knee_roll" gear="150" ctrlrange="-1 1"/>
    <motor name="r_ankle_pitch_motor" joint="r_ankle_pitch" gear="200" ctrlrange="-1 1"/>
    <motor name="r_ankle_roll_motor" joint="r_ankle_roll" gear="150" ctrlrange="-1 1"/>
    <motor name="r_ankle_yaw_motor" joint="r_ankle_yaw" gear="100" ctrlrange="-1 1"/>
  </actuator>

  <!-- ============================================================= -->
  <!-- SENSORS -->
  <!-- ============================================================= -->
  <sensor>
    <!-- IMU sensors -->
    <accelerometer name="imu_acc" site="imu_site"/>
    <gyro name="imu_gyro" site="imu_site"/>
    <framequat name="imu_quat" objtype="site" objname="imu_site"/>

    <!-- Contact sensors -->
    <touch name="l_foot_touch" site="l_foot_site"/>
