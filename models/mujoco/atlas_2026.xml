  Boston Dynamics Atlas 2026 - MuJoCo Model
  56 DOF Fully Rotational Humanoid Robot
  Based on CES 2026 announcement specifications

  Author: Al Numan
-->
<mujoco model="atlas_2026">
  <compiler angle="radian" meshdir="meshes/" autolimits="true"/>

  <option timestep="0.0005" iterations="50" solver="Newton" gravity="0 0 -9.81">
    <flag warmstart="enable"/>
  </option>

  <size nconmax="100" njmax="500"/>

  <default>
    <joint armature="0.01" damping="1" limited="true"/>
    <geom condim="4" friction="0.8 0.02 0.01" margin="0.001"/>
    <motor ctrllimited="true"/>
    <position kp="100" kv="10"/>

    <!-- Default classes for different body parts -->
    <default class="hip">
      <joint damping="8.0" armature="0.1"/>
      <geom friction="0.8 0.02 0.01"/>
    </default>

    <default class="knee">
      <joint damping="10.0" armature="0.08"/>
    </default>

    <default class="ankle">
      <joint damping="5.0" armature="0.05"/>
    </default>

    <default class="torso">
      <joint damping="5.0" armature="0.1"/>
    </default>

    <default class="arm">
      <joint damping="4.0" armature="0.05"/>
    </default>

    <default class="wrist">
      <joint damping="2.0" armature="0.02"/>
    </default>

    <default class="hand">
      <joint damping="0.5" armature="0.005"/>
    </default>

    <default class="neck">
