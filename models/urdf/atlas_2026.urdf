<?xml version="1.0"?>
<!--
  Boston Dynamics Atlas 2026 - Production Version URDF
  56 DOF Fully Rotational Humanoid Robot
  Based on CES 2026 announcement specifications

  Author: Al Numan
-->
<robot name="atlas_2026">

  <!-- ============================================================= -->
  <!-- MATERIALS -->
  <!-- ============================================================= -->
  <material name="atlas_dark_grey">
    <color rgba="0.2 0.2 0.2 1.0"/>
  </material>
  <material name="atlas_light_grey">
    <color rgba="0.6 0.6 0.6 1.0"/>
  </material>
  <material name="atlas_orange">
    <color rgba="1.0 0.4 0.0 1.0"/>
  </material>
  <material name="atlas_white">
    <color rgba="0.9 0.9 0.9 1.0"/>
  </material>

  <!-- ============================================================= -->
  <!-- BASE LINK (PELVIS) -->
  <!-- ============================================================= -->
  <link name="pelvis">
    <visual>
      <geometry>
        <box size="0.28 0.32 0.20"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <material name="atlas_dark_grey"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.28 0.32 0.20"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="15.0"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.3" ixy="0" ixz="0" iyy="0.35" iyz="0" izz="0.25"/>
    </inertial>
  </link>

  <!-- ============================================================= -->
  <!-- TORSO/SPINE (6 DOF) - Lower Spine (Waist) -->
  <!-- ============================================================= -->

  <!-- Waist Yaw Joint -->
  <joint name="waist_yaw" type="revolute">
    <parent link="pelvis"/>
    <child link="waist_yaw_link"/>
    <origin xyz="0 0 0.12" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.57" upper="1.57" effort="400" velocity="6.0"/>
    <dynamics damping="5.0" friction="1.0"/>
  </joint>

  <link name="waist_yaw_link">
    <visual>
      <geometry>
        <cylinder radius="0.08" length="0.06"/>
      </geometry>
      <material name="atlas_light_grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.08" length="0.06"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2.0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <!-- Waist Pitch Joint -->
  <joint name="waist_pitch" type="revolute">
    <parent link="waist_yaw_link"/>
    <child link="waist_pitch_link"/>
    <origin xyz="0 0 0.04" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-0.52" upper="0.70" effort="400" velocity="6.0"/>
    <dynamics damping="5.0" friction="1.0"/>
  </joint>

  <link name="waist_pitch_link">
    <visual>
      <geometry>
        <cylinder radius="0.08" length="0.06"/>
      </geometry>
      <material name="atlas_light_grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.08" length="0.06"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2.0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <!-- Waist Roll Joint -->
  <joint name="waist_roll" type="revolute">
    <parent link="waist_pitch_link"/>
    <child link="torso_link"/>
    <origin xyz="0 0 0.04" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.44" upper="0.44" effort="400" velocity="6.0"/>
    <dynamics damping="5.0" friction="1.0"/>
  </joint>

  <link name="torso_link">
    <visual>
      <geometry>
        <box size="0.30 0.40 0.30"/>
      </geometry>
      <origin xyz="0 0 0.15" rpy="0 0 0"/>
      <material name="atlas_dark_grey"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.30 0.40 0.30"/>
      </geometry>
      <origin xyz="0 0 0.15" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="18.0"/>
      <origin xyz="0 0 0.15" rpy="0 0 0"/>
      <inertia ixx="0.6" ixy="0" ixz="0" iyy="0.7" iyz="0" izz="0.5"/>
    </inertial>
  </link>

  <!-- ============================================================= -->
  <!-- TORSO/SPINE - Upper Spine (Chest) -->
  <!-- ============================================================= -->

  <!-- Chest Yaw Joint -->
  <joint name="chest_yaw" type="revolute">
    <parent link="torso_link"/>
    <child link="chest_yaw_link"/>
    <origin xyz="0 0 0.32" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.05" upper="1.05" effort="300" velocity="6.0"/>
    <dynamics damping="4.0" friction="0.8"/>
  </joint>

  <link name="chest_yaw_link">
    <visual>
      <geometry>
        <cylinder radius="0.10" length="0.06"/>
      </geometry>
      <material name="atlas_light_grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.10" length="0.06"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2.5"/>
      <inertia ixx="0.015" ixy="0" ixz="0" iyy="0.015" iyz="0" izz="0.02"/>
    </inertial>
  </link>

  <!-- Chest Pitch Joint -->
  <joint name="chest_pitch" type="revolute">
    <parent link="chest_yaw_link"/>
    <child link="chest_pitch_link"/>
    <origin xyz="0 0 0.04" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-0.35" upper="0.52" effort="300" velocity="6.0"/>
    <dynamics damping="4.0" friction="0.8"/>
  </joint>

  <link name="chest_pitch_link">
    <visual>
      <geometry>
        <cylinder radius="0.10" length="0.06"/>
      </geometry>
      <material name="atlas_light_grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.10" length="0.06"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2.5"/>
      <inertia ixx="0.015" ixy="0" ixz="0" iyy="0.015" iyz="0" izz="0.02"/>
    </inertial>
  </link>

  <!-- Chest Roll Joint -->
  <joint name="chest_roll" type="revolute">
    <parent link="chest_pitch_link"/>
    <child link="chest_link"/>
    <origin xyz="0 0 0.04" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.35" upper="0.35" effort="300" velocity="6.0"/>
    <dynamics damping="4.0" friction="0.8"/>
  </joint>

  <link name="chest_link">
    <visual>
      <geometry>
        <box size="0.32 0.52 0.25"/>
      </geometry>
      <origin xyz="0 0 0.125" rpy="0 0 0"/>
      <material name="atlas_dark_grey"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.32 0.52 0.25"/>
      </geometry>
      <origin xyz="0 0 0.125" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="12.0"/>
      <origin xyz="0 0 0.125" rpy="0 0 0"/>
      <inertia ixx="0.4" ixy="0" ixz="0" iyy="0.5" iyz="0" izz="0.35"/>
    </inertial>
  </link>

  <!-- ============================================================= -->
  <!-- HEAD/NECK (3 DOF) -->
  <!-- ============================================================= -->

  <!-- Neck Yaw -->
  <joint name="neck_yaw" type="revolute">
    <parent link="chest_link"/>
    <child link="neck_yaw_link"/>
    <origin xyz="0 0 0.28" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.57" upper="1.57" effort="30" velocity="4.0"/>
    <dynamics damping="1.0" friction="0.2"/>
  </joint>

  <link name="neck_yaw_link">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.04"/>
      </geometry>
      <material name="atlas_light_grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.05" length="0.04"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <!-- Neck Pitch -->
  <joint name="neck_pitch" type="revolute">
    <parent link="neck_yaw_link"/>
    <child link="neck_pitch_link"/>
    <origin xyz="0 0 0.03" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-0.70" upper="0.87" effort="30" velocity="4.0"/>
    <dynamics damping="1.0" friction="0.2"/>
  </joint>

  <link name="neck_pitch_link">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.04"/>
      </geometry>
      <material name="atlas_light_grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.05" length="0.04"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <!-- Neck Roll -->
  <joint name="neck_roll" type="revolute">
    <parent link="neck_pitch_link"/>
    <child link="head_link"/>
    <origin xyz="0 0 0.03" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.52" upper="0.52" effort="30" velocity="4.0"/>
    <dynamics damping="1.0" friction="0.2"/>
  </joint>

  <link name="head_link">
    <visual>
      <geometry>
        <box size="0.18 0.16 0.22"/>
      </geometry>
      <origin xyz="0 0 0.11" rpy="0 0 0"/>
      <material name="atlas_white"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.18 0.16 0.22"/>
      </geometry>
      <origin xyz="0 0 0.11" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="3.0"/>
      <origin xyz="0 0 0.11" rpy="0 0 0"/>
      <inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.015"/>
    </inertial>
  </link>

  <!-- IMU Link (fixed to head) -->
  <joint name="imu_joint" type="fixed">
    <parent link="pelvis"/>
    <child link="imu_link"/>
    <origin xyz="0 0 0.05" rpy="0 0 0"/>
  </joint>

  <link name="imu_link">
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

  <!-- ============================================================= -->
  <!-- LEFT LEG (8 DOF) -->
  <!-- ============================================================= -->

  <!-- Left Hip Yaw -->
  <joint name="l_hip_yaw" type="revolute">
    <parent link="pelvis"/>
    <child link="l_hip_yaw_link"/>
    <origin xyz="0 0.16 -0.10" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14" upper="3.14" effort="350" velocity="12.0"/>
    <dynamics damping="8.0" friction="2.0"/>
  </joint>

  <link name="l_hip_yaw_link">
    <visual>
      <geometry>
        <cylinder radius="0.06" length="0.08"/>
      </geometry>
      <material name="atlas_light_grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.06" length="0.08"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2.0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.008"/>
    </inertial>
  </link>

  <!-- Left Hip Roll -->
  <joint name="l_hip_roll" type="revolute">
    <parent link="l_hip_yaw_link"/>
    <child link="l_hip_roll_link"/>
    <origin xyz="0 0 -0.05" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.70" upper="0.70" effort="350" velocity="12.0"/>
    <dynamics damping="8.0" friction="2.0"/>
  </joint>

  <link name="l_hip_roll_link">
    <visual>
      <geometry>
        <cylinder radius="0.06" length="0.08"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <material name="atlas_light_grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.06" length="0.08"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
    </collision>
    <inertial>
      <mass value="2.0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.008" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <!-- Left Hip Pitch -->
  <joint name="l_hip_pitch" type="revolute">
    <parent link="l_hip_roll_link"/>
    <child link="l_thigh_link"/>
    <origin xyz="0 0 -0.05" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.74" upper="0.87" effort="450" velocity="12.0"/>
    <dynamics damping="10.0" friction="2.5"/>
  </joint>

  <link name="l_thigh_link">
    <visual>
      <geometry>
        <box size="0.10 0.12 0.40"/>
      </geometry>
      <origin xyz="0 0 -0.20" rpy="0 0 0"/>
      <material name="atlas_dark_grey"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.10 0.12 0.40"/>
      </geometry>
      <origin xyz="0 0 -0.20" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="6.0"/>
      <origin xyz="0 0 -0.20" rpy="0 0 0"/>
      <inertia ixx="0.12" ixy="0" ixz="0" iyy="0.12" iyz="0" izz="0.03"/>
    </inertial>
  </link>

  <!-- Left Knee Pitch -->
  <joint name="l_knee_pitch" type="revolute">
    <parent link="l_thigh_link"/>
    <child link="l_knee_pitch_link"/>
    <origin xyz="0 0 -0.42" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="0.0" upper="2.62" effort="500" velocity="12.0"/>
    <dynamics damping="10.0" friction="2.5"/>
  </joint>

  <link name="l_knee_pitch_link">
    <visual>
      <geometry>
        <cylinder radius="0.06" length="0.10"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <material name="atlas_orange"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.06" length="0.10"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
    </collision>
    <inertial>
      <mass value="2.5"/>
      <inertia ixx="0.015" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.015"/>
    </inertial>
  </link>

  <!-- Left Knee Roll (unique to Atlas) -->
  <joint name="l_knee_roll" type="revolute">
    <parent link="l_knee_pitch_link"/>
    <child link="l_shank_link"/>
    <origin xyz="0 0 -0.02" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.26" upper="0.26" effort="150" velocity="12.0"/>
    <dynamics damping="5.0" friction="1.0"/>
  </joint>

  <link name="l_shank_link">
    <visual>
      <geometry>
        <box size="0.09 0.10 0.38"/>
      </geometry>
      <origin xyz="0 0 -0.19" rpy="0 0 0"/>
      <material name="atlas_dark_grey"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.09 0.10 0.38"/>
      </geometry>
      <origin xyz="0 0 -0.19" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="4.5"/>
      <origin xyz="0 0 -0.19" rpy="0 0 0"/>
      <inertia ixx="0.08" ixy="0" ixz="0" iyy="0.08" iyz="0" izz="0.02"/>
    </inertial>
  </link>

  <!-- Left Ankle Pitch -->
  <joint name="l_ankle_pitch" type="revolute">
    <parent link="l_shank_link"/>
    <child link="l_ankle_pitch_link"/>
    <origin xyz="0 0 -0.40" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.05" upper="0.70" effort="200" velocity="12.0"/>
    <dynamics damping="6.0" friction="1.5"/>
  </joint>

  <link name="l_ankle_pitch_link">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.06"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <material name="atlas_light_grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.05" length="0.06"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
    </collision>
    <inertial>
      <mass value="1.5"/>
      <inertia ixx="0.005" ixy="0" ixz="0" iyy="0.004" iyz="0" izz="0.005"/>
    </inertial>
  </link>

  <!-- Left Ankle Roll -->
  <joint name="l_ankle_roll" type="revolute">
    <parent link="l_ankle_pitch_link"/>
    <child link="l_ankle_roll_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.44" upper="0.44" effort="150" velocity="12.0"/>
    <dynamics damping="5.0" friction="1.0"/>
  </joint>

  <link name="l_ankle_roll_link">
    <visual>
      <geometry>
        <cylinder radius="0.04" length="0.04"/>
      </geometry>
      <material name="atlas_light_grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.04" length="0.04"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <inertia ixx="0.002" ixy="0" ixz="0" iyy="0.002" iyz="0" izz="0.002"/>
    </inertial>
  </link>

  <!-- Left Ankle Yaw -->
  <joint name="l_ankle_yaw" type="revolute">
    <parent link="l_ankle_roll_link"/>
    <child link="l_foot_link"/>
    <origin xyz="0 0 -0.03" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.52" upper="0.52" effort="100" velocity="12.0"/>
    <dynamics damping="4.0" friction="0.8"/>
  </joint>

  <link name="l_foot_link">
    <visual>
      <geometry>
        <box size="0.28 0.12 0.05"/>
      </geometry>
      <origin xyz="0.05 0 -0.025" rpy="0 0 0"/>
      <material name="atlas_orange"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.28 0.12 0.05"/>
      </geometry>
      <origin xyz="0.05 0 -0.025" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="1.5"/>
      <origin xyz="0.05 0 -0.025" rpy="0 0 0"/>
      <inertia ixx="0.003" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <!-- ============================================================= -->
  <!-- RIGHT LEG (8 DOF) - Mirror of left leg -->
  <!-- ============================================================= -->

  <!-- Right Hip Yaw -->
  <joint name="r_hip_yaw" type="revolute">
    <parent link="pelvis"/>
    <child link="r_hip_yaw_link"/>
    <origin xyz="0 -0.16 -0.10" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14" upper="3.14" effort="350" velocity="12.0"/>
    <dynamics damping="8.0" friction="2.0"/>
  </joint>

  <link name="r_hip_yaw_link">
    <visual>
      <geometry>
        <cylinder radius="0.06" length="0.08"/>
      </geometry>
      <material name="atlas_light_grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.06" length="0.08"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2.0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.008"/>
    </inertial>
  </link>

  <!-- Right Hip Roll -->
  <joint name="r_hip_roll" type="revolute">
    <parent link="r_hip_yaw_link"/>
    <child link="r_hip_roll_link"/>
    <origin xyz="0 0 -0.05" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.70" upper="0.70" effort="350" velocity="12.0"/>
    <dynamics damping="8.0" friction="2.0"/>
  </joint>

  <link name="r_hip_roll_link">
    <visual>
      <geometry>
        <cylinder radius="0.06" length="0.08"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <material name="atlas_light_grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.06" length="0.08"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
    </collision>
    <inertial>
      <mass value="2.0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.008" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <!-- Right Hip Pitch -->
  <joint name="r_hip_pitch" type="revolute">
    <parent link="r_hip_roll_link"/>
    <child link="r_thigh_link"/>
    <origin xyz="0 0 -0.05" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.74" upper="0.87" effort="450" velocity="12.0"/>
    <dynamics damping="10.0" friction="2.5"/>
  </joint>

  <link name="r_thigh_link">
    <visual>
      <geometry>
        <box size="0.10 0.12 0.40"/>
      </geometry>
      <origin xyz="0 0 -0.20" rpy="0 0 0"/>
      <material name="atlas_dark_grey"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.10 0.12 0.40"/>
      </geometry>
      <origin xyz="0 0 -0.20" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="6.0"/>
      <origin xyz="0 0 -0.20" rpy="0 0 0"/>
      <inertia ixx="0.12" ixy="0" ixz="0" iyy="0.12" iyz="0" izz="0.03"/>
    </inertial>
  </link>

  <!-- Right Knee Pitch -->
  <joint name="r_knee_pitch" type="revolute">
    <parent link="r_thigh_link"/>
